//concatenate the two string without using the strcat() function

#include<stdio.h>
#include<string.h>
int main(){
printf("first name ");
char n1[20];
scanf("%s",n1);


printf("second name ");
char n2[20];
scanf("%s",n2);


int len1=strlen(n1);
int len2=strlen(n2);

for(int i=0;i<=len2;i++){
    n1[len1+i]=n2[i];
}
printf("concatenating the name ");
printf(n1);

}
