// compare two string without using the strcmp()

#include<stdio.h>
#include<string.h>
int main(){
printf("enter the name");
char a[20];
scanf("%s",a);


printf("enter the another name");
char b[20];
scanf("%s",b);
int flag=0;
for(int i=0; a[i]!='\0' || b[i]!='\0';i++){
    if(a[i]!=b[i]){
            flag=1;
        break;
    }
}
if(flag==1){
    printf("not match");
}
}
